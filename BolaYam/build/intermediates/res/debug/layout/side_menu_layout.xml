<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:id="@+id/user_image_container"
        android:layout_width="match_parent"
        android:layout_height="110dp" >

        <com.application.ui.RoundedImageView
            android:id="@+id/user_image"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:scaleType="centerCrop"
            app:border_color="#118cff"
            app:border_width="1dp"
            app:oval="true" />

        <ImageView
            android:id="@+id/beach_tree"
            android:layout_width="46dp"
            android:layout_height="70dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:src="@drawable/beach_entry_right_tree" />
    </RelativeLayout>

    <ViewStub
        android:id="@+id/drawer_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</merge><!-- From: file:/C:/Users/elad/Desktop/project/BolaYamAppAndroidStudio/BolaYam/res/layout/side_menu_layout.xml -->